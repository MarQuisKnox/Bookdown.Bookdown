{
    "name": "bookdown/bookdown",
    "type": "library",
    "description": "Provides DocBook-like rendering of Markdown files.",
    "keywords": [
        "docbook",
        "markdown",
        "manual",
        "documentation",
        "static site"
    ],
    "homepage": "https://github.com/bookdown/bookdown",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Bookdown Contributors",
            "homepage": "https://github.com/bookdown/bookdown/contributors"
        }
    ],
    "require": {
        "aura/cli": "~2.0",
        "aura/html": "~2.0",
        "aura/view": "~2.0",
        "league/commonmark": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "Bookdown\\Bookdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bookdown\\Bookdown\\": "tests/"
        }
    },
    "bin": [
        "bin/bookdown.php"
    ]
}
